{
 "printWidth": 80,
 "tabWidth": 4,
 "useTabs": false,
 "semi": true,
 "singleQuote": false,
 "jsxSingleQuote": false,
 "trailingComma": "none",
 "bracketSpacing": true,
 "arrowParens": "always",
 "quoteProps": "as-needed",
 "htmlWhitespaceSensitivity": "css",
 "endOfLine": "lf",
 "plugins": ["prettier-plugin-tailwindcss"]
}
